{
  "parser": "@typescript-eslint/parser",
  "ignorePatterns": ["build/", "node_modules/", "src/serviceWorker.js"],
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/destructuring-assignment": 0,
    "indent": ["error", 2]
  }
}